{
    "name": "erikaheidi/autodocs",
    "description": "Reference Documentation Builder",
    "license": "MIT",
    "homepage": "https://github.com/erikaheidi/autodocs",
    "keywords": ["documentation", "cli"],
    "autoload": {
        "psr-4": {
            "Autodocs\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "minicli/minicli": "^4.2",
        "minicli/stencil": "^0.2.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.16",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
